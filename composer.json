{
  "name": "php-yaoi/php-yaoi",
  "type": "library",
  "description": "Yaoi is yet another object interleaver",
  "keywords": ["framework"],
  "homepage": "https://github.com/php-yaoi/php-yaoi",
  "license": "MIT",
  "authors": [
    {
      "name": "Viacheslav Poturaev",
      "email": "vearutop@gmail.com",
      "homepage": "https://github.com/vearutop"
    }
  ],
  "require": {
    "php": ">=5.3.0"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "dev-master",
    "phpunit/phpunit": "~4.0",
    "psr/log": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Yaoi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "YaoiTests\\": "tests/src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
